{
    "name": "chikovani/commission-calculator",
    "description": "Calculates commission fees for financial operations.",
    "type": "project",
    "require": {
        "php": ">=7.4 || ^8.0",
      "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs --standard=PSR12 src tests bin"
    }

}
